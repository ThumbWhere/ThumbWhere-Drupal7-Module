/**
 * Form callback: confirmation form for deleting a thumbwhere_member.
 *
 * @param $thumbwhere_member
 *   The thumbwhere_member to delete
 *
 * @see confirm_form()
 */
function thumbwhere_member_delete_form($form, &$form_state, $thumbwhere_member) {
  $form_state['thumbwhere_member'] = $thumbwhere_member;

  $form['#submit'][] = 'thumbwhere_member_delete_form_submit';

  $form = confirm_form($form,
    t('Are you sure you want to delete thumbwhere_member %name?', array('%name' => $thumbwhere_member->pk_member)),
    'admin/thumbwhere/thumbwhere_members/thumbwhere_member',
    '<p>' . t('This action cannot be undone.') . '</p>',
    t('Delete'),
    t('Cancel'),
    'confirm'
  );

  return $form;
}

/**
 * Submit callback for thumbwhere_member_delete_form
 */
function thumbwhere_member_delete_form_submit($form, &$form_state) {
  $thumbwhere_member = $form_state['thumbwhere_member'];

  thumbwhere_member_delete($thumbwhere_member);

  drupal_set_message(t('The thumbwhere_member %name has been deleted.', array('%name' => $thumbwhere_member->pk_member)));
  watchdog('thumbwhere_member', 'Deleted thumbwhere_member %name.', array('%name' => $thumbwhere_member->pk_member));

  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_members';
}




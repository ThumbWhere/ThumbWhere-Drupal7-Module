/**
 * Form API submit callback for the thumbwhere_action form.
 *
 * @todo remove hard-coded link
 */
function thumbwhere_action_edit_form_submit(&$form, &$form_state) {

  $thumbwhere_action = entity_ui_controller('thumbwhere_action')->entityFormSubmitBuildEntity($form, $form_state);
  // Save the thumbwhere_action and go back to the list of thumbwhere_actions

  // Add in created and changed times.
  if ($thumbwhere_action->is_new = isset($thumbwhere_action->is_new) ? $thumbwhere_action->is_new : 0) {
    $thumbwhere_action->created = time();
  }

  $thumbwhere_action->changed = time();

  $thumbwhere_action->save();
  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_actions';
}

/**
 * Form API submit callback for the delete button.
 *
 * @todo Remove hard-coded path
 */
function thumbwhere_action_form_submit_delete(&$form, &$form_state) {
  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_actions/thumbwhere_action/' . $form_state['thumbwhere_action']->pk_action . '/delete';
}





/**
 * Form callback: confirmation form for deleting a thumbwhere_action.
 *
 * @param $thumbwhere_action
 *   The thumbwhere_action to delete
 *
 * @see confirm_form()
 */
function thumbwhere_action_delete_form($form, &$form_state, $thumbwhere_action) {
  $form_state['thumbwhere_action'] = $thumbwhere_action;

  $form['#submit'][] = 'thumbwhere_action_delete_form_submit';

  $form = confirm_form($form,
    t('Are you sure you want to delete thumbwhere_action %name?', array('%name' => $thumbwhere_action->pk_action)),
    'admin/thumbwhere/thumbwhere_actions/thumbwhere_action',
    '<p>' . t('This action cannot be undone.') . '</p>',
    t('Delete'),
    t('Cancel'),
    'confirm'
  );

  return $form;
}

/**
 * Submit callback for thumbwhere_action_delete_form
 */
function thumbwhere_action_delete_form_submit($form, &$form_state) {
  $thumbwhere_action = $form_state['thumbwhere_action'];

  thumbwhere_action_delete($thumbwhere_action);

  drupal_set_message(t('The thumbwhere_action %name has been deleted.', array('%name' => $thumbwhere_action->pk_action)));
  watchdog('thumbwhere_action', 'Deleted thumbwhere_action %name.', array('%name' => $thumbwhere_action->pk_action));

  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_actions';
}




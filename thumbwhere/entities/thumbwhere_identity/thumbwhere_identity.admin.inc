/**
 * Form callback: confirmation form for deleting a thumbwhere_identity.
 *
 * @param $thumbwhere_identity
 *   The thumbwhere_identity to delete
 *
 * @see confirm_form()
 */
function thumbwhere_identity_delete_form($form, &$form_state, $thumbwhere_identity) {
  $form_state['thumbwhere_identity'] = $thumbwhere_identity;

  $form['#submit'][] = 'thumbwhere_identity_delete_form_submit';

  $form = confirm_form($form,
    t('Are you sure you want to delete thumbwhere_identity %name?', array('%name' => $thumbwhere_identity->id)),
    'admin/thumbwhere/thumbwhere_identitys/thumbwhere_identity',
    '<p>' . t('This action cannot be undone.') . '</p>',
    t('Delete'),
    t('Cancel'),
    'confirm'
  );

  return $form;
}

/**
 * Submit callback for thumbwhere_identity_delete_form
 */
function thumbwhere_identity_delete_form_submit($form, &$form_state) {
  $thumbwhere_identity = $form_state['thumbwhere_identity'];

  thumbwhere_identity_delete($thumbwhere_identity);

  drupal_set_message(t('The thumbwhere_identity %name has been deleted.', array('%name' => $thumbwhere_identity->id)));
  watchdog('thumbwhere_identity', 'Deleted thumbwhere_identity %name.', array('%name' => $thumbwhere_identity->id));

  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_identitys';
}




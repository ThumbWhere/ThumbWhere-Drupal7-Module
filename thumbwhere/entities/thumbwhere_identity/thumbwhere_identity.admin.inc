function thumbwhere_identity_edit_form_submit(&$form, &$form_state) {

  $thumbwhere_identity = entity_ui_controller('thumbwhere_identity')->entityFormSubmitBuildEntity($form, $form_state);
  // Save the thumbwhere_identity and go back to the list of thumbwhere_identitys

  // Add in created and changed times.
  if ($thumbwhere_identity->is_new = isset($thumbwhere_identity->is_new) ? $thumbwhere_identity->is_new : 0) {
    $thumbwhere_identity->created = time();
  }

  $thumbwhere_identity->changed = time();

  $thumbwhere_identity->save();
  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_identitys';
}


 *
 * @param $thumbwhere_originsync
 *   The thumbwhere_originsync to delete
 *
 * @see confirm_form()
 */
function thumbwhere_originsync_delete_form($form, &$form_state, $thumbwhere_originsync) {
  $form_state['thumbwhere_originsync'] = $thumbwhere_originsync;

  $form['#submit'][] = 'thumbwhere_originsync_delete_form_submit';

  $form = confirm_form($form,
    t('Are you sure you want to delete thumbwhere_originsync %name?', array('%name' => $thumbwhere_originsync->pk_originsync)),
    'admin/thumbwhere/thumbwhere_originsyncs/thumbwhere_originsync',
    '<p>' . t('This action cannot be undone.') . '</p>',
    t('Delete'),
    t('Cancel'),
    'confirm'
  );

  return $form;
}

/**
 * Submit callback for thumbwhere_originsync_delete_form
 */
function thumbwhere_originsync_delete_form_submit($form, &$form_state) {
  $thumbwhere_originsync = $form_state['thumbwhere_originsync'];

  thumbwhere_originsync_delete($thumbwhere_originsync);

  drupal_set_message(t('The thumbwhere_originsync %name has been deleted.', array('%name' => $thumbwhere_originsync->pk_originsync)));
  watchdog('thumbwhere_originsync', 'Deleted thumbwhere_originsync %name.', array('%name' => $thumbwhere_originsync->pk_originsync));

  $form_state['redirect'] = 'admin/thumbwhere/thumbwhere_originsyncs';
}



